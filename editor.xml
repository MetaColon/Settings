<application>
  <component name="CodeInsightSettings">
    <option name="AUTOCOMPLETE_ON_CODE_COMPLETION" value="false" />
    <option name="REFORMAT_ON_PASTE" value="4" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="question" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;TODO)(\W|$)(.*)" />
    <pattern icon="question" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;ENHANCEMENT)(\W|$)(.*)" />
    <pattern icon="important" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;BUG)(\W|$)(.*)" />
    <pattern icon="question" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;VALIDATE)(\W|$)(.*)" />
    <pattern pattern="\bNotImplementedException\b" />
  </component>
</application>