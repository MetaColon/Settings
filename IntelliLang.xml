<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="Latex" injector-id="xml">
      <display-name>blockquote</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("blockquote")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
  </component>
</application>